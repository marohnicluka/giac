    <|unfolded-io>
      <\equation*>
        <around*|[|<frac|5|8>,<frac|5|8>,<frac|5|8>,<frac|5|7>,<frac|5|8>,1|]>
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
