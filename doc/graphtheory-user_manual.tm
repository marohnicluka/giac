    <|unfolded-io>
      <\equation*>
        <text|a directed weighted graph with 6 vertices and 9 arcs>
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      draw_graph(DG,spring)
    <|unfolded-io>
      <htab|><image|<tuple|<#>|pdf>|0.618par|||><htab|>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      kspaths(DG,C,H,5)
    <|unfolded-io>
      <\equation*>
        <around*|[|<around*|[|C,E,F,H|]>,<around*|[|C,E,G,H|]>,<around*|[|C,E,D,F,H|]>,<around*|[|C,D,F,H|]>,<around*|[|C,E,F,G,H|]>|]>
      </equation*>
    </unfolded-io>
