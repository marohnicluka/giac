      <htab|><image|<tuple|<#>|pdf>|0.618par|||><htab|>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      E:=[[1,4],[3,4]]
    <|unfolded-io>
      <\equation*>
        <matrix|<tformat|<table|<row|<cell|1>|<cell|4>>|<row|<cell|3>|<cell|4>>>>>
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      is_cut_set(G,E)
    <|unfolded-io>
