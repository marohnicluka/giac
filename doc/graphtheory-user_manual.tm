      <\equation*>
        <text|an undirected unweighted graph with 9 vertices and 11 edges>
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      draw_graph(G)
    <|unfolded-io>
      <htab|><image|<tuple|<#>|pdf>|0.618par|||><htab|>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      vertex_degree(G,1)
    <|unfolded-io>
      <\equation*>
        5
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      degree_sequence(G)
    <|unfolded-io>
      <\equation*>
        <around*|[|5,2,2,3,2,2,2,2,2|]>
      </equation*>
    </unfolded-io>

