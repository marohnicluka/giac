      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      has_arc(D,%{1,2%})
    <|unfolded-io>
      <\equation*>
        true
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
    <|unfolded-io>
      has_arc(D,[4,5])
    <|unfolded-io>
      <\equation*>
        true
      </equation*>
    </unfolded-io>

    <\unfolded-io>
      \<gtr\>\ 
